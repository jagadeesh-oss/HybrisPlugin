<div class="unbxd-search-results" style="display:none">
    <style>.search-empty {display:none}</style>
<div class="row">
    <div class="col-xs-3">
        <div class="yCmsContentSlot search-list-page-left-refinements-slot">
            <div class="yCmsComponent search-list-page-left-refinements-component">
                <div id="selected-product-facet" class="hidden-sm hidden-xs product__facet js-product-facet">
                </div>
                <div id="product-facet" class="hidden-sm hidden-xs product__facet js-product-facet">
                </div>
            </div>
        </div>
    </div>
    <div class="col-sm-12 col-md-9">
        <div class="yCmsContentSlot search-list-page-right-result-list-slot">
            <div class="yCmsComponent search-list-page-right-result-list-component">
                <div class="product__list--wrapper">
                    <div class="header-container clearfix display-none">
                        <div class="header row clearfix">
                            <div class="header-search">
                                <div class="search-input-button-holder clearfix">
                                    <form method="GET" action="">
                                        <input type="text" class="search-input lt" id="search_input" unbxdattr="sq"
                                               name="q" autoComplete="off" placeholder="Search..."/>
                                        <button type="submit" class="search-button lt" id="search_button"
                                                unbxdattr="sq_bt"></button>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="results">
                        <h1 id="search_title"></h1>
                    </div>
                    <div class="pagination-bar top">
                        <div class="pagination-toolbar">
                            <div class="helper clearfix hidden-md hidden-lg"></div>
                            <div class="sort-refine-bar">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-2   col-md-2">
                                        <div class="form-group">
                                            <label class="control-label " for="sortForm1">Sort by:</label>
                                            <form class="sortForm" id="sortForm1" name="sortForm1" method="get"
                                                  action="#">

                                            </form>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-2 col-md-2">
                                        <div class="form-group">
                                            <label class="control-label " for="limitForm1">Results per page:</label>
                                            <form class="limitForm" id="limitForm1" name="limitForm1" method="get"
                                                  action="#">

                                            </form>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-2 col-md-2">
                                        <div class="search-header-options">
                                            <div class="change-view">
                                                <label class="control-label ">View by:</label>
                                                <div class="view-types">
                                                        <span class="list-view">
                                                            List View
                                                        </span>
                                                    <span class="grid-view">
                                                            Grid View
                                                        </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-6 col-md-5 pagination-wrap">
                                        <ul class="pagination">

                                        </ul>
                                    </div>

                                    <div class="col-xs-12 col-sm-2 col-md-4 hidden-md hidden-lg">
                                        <button class="btn btn-default js-show-facets"
                                                data-select-refinements-title="Select Refinements">
                                            Refine
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="pagination-bar-results"></div>
                            </div>
                        </div>
                    </div>
                    <ul class="product__listing product__list"></ul>
                    <div id="addToCartTitle" class="display-none">
                        <div class="add-to-cart-header">
                            <div class="headline">
                                <span class="headline-text">Added to Your Shopping Cart</span>
                            </div>
                        </div>
                    </div>
                    <div class="pagination-bar bottom">
                        <div class="pagination-toolbar">
                            <div class="helper clearfix hidden-md hidden-lg"></div>
                            <div class="sort-refine-bar">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-4 col-md-4">
                                        <div class="form-group">
                                            <label class="control-label " for="sortForm2">Sort by:</label>
                                            <form class="sortForm" id="sortForm2" name="sortForm2" method="get"
                                                  action="#">

                                            </form>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label " for="limitForm2">Results per page:</label>
                                            <form class="limitForm" id="limitForm2" name="limitForm2" method="get"
                                                  action="#">

                                            </form>
                                        </div>
                                        <div class="search-header-options">
                                            <div class="change-view">
                                                <label class="control-label ">View by:</label>
                                                <div class="view-types">
                                                        <span class="list-view">
                                                            List View
                                                        </span>
                                                    <span class="grid-view">
                                                            Grid View
                                                        </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-6 col-md-5 pagination-wrap">
                                        <ul class="pagination">

                                        </ul>
                                    </div>

                                    <div class="col-xs-12 col-sm-2 col-md-4 hidden-md hidden-lg">
                                        <button class="btn btn-default js-show-facets"
                                                data-select-refinements-title="Select Refinements">
                                            Refine
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="pagination-bar-results"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>


