<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="items.xsd">
    <collectiontypes>
        <collectiontype code="UnbxdIndexerJobList" elementtype="UnbxdIndexerCronJob" type="list"/>
    </collectiontypes>
    <enumtypes>
        <enumtype generate="true" code="UnbxdUploadStatus"
                  autocreate="true" dynamic="true">
            <description>This type determines status of unbxd upload.</description>
            <value code="INDEXING" />
            <value code="INDEXED" />
            <value code="FAILED" />
        </enumtype>
        <enumtype generate="true" code="UnbxdDataType" autocreate="true" dynamic="true">
            <value code="text"/>
            <value code="decimal"/>
            <value code="number"/>
            <value code="descriptiontext"/>
            <value code="link"/>
            <value code="date"/>
            <value code="bool"/>
            <value code="nsku"/>
            <value code="facet"/>
            <value code="sku"/>
            <value code="path"/>
        </enumtype>
    </enumtypes>
    <itemtypes>
