        <enumtype generate="true" code="UnbxdUploadStatus"
                  autocreate="true" dynamic="true">
            <description>This type determines status of unbxd upload.</description>
            <value code="INDEXING" />
            <value code="INDEXED" />
            <value code="FAILED" />
        </enumtype>
    </enumtypes>
