        <itemtype code="SolrIndexedType" generate="false" autocreate="false">
            <attributes>
                <attribute qualifier="isUnbxd" type="java.lang.Boolean">
                    <description>If index is for Unbxd</description>
                    <persistence type="property"/>
                    <modifiers read="true" write="true" optional="false"/>
                    <defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="SolrIndexedProperty" generate="false" autocreate="false">
            <attributes>
                <attribute qualifier="isUnbxd" type="java.lang.Boolean">
                    <description>If index property is relevant for Unbxd</description>
                    <persistence type="property"/>
                    <modifiers read="true" write="true" optional="false"/>
                    <defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
                </attribute>
                <attribute qualifier="unbxdType" type="UnbxdDataType">
                    <description>Index property type Unbxd</description>
                    <persistence type="property"/>
                    <modifiers read="true" write="true" optional="true"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="Product" generate="false" autocreate="false">
            <attributes>
                <attribute qualifier="unbxdSyncDate" type="java.util.Date">
                    <modifiers read="true" write="true" search="false" optional="true"/>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="UnbxdUploadTask" extends="GenericItem" autocreate="true" generate="true">
            <deployment table="unbxduploadtasks" typecode="10300"/>
            <attributes>
                <attribute type="java.lang.String" qualifier="fileName">
                    <modifiers read="true" write="true"  optional="false" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.String" qualifier="uploadId">
                    <modifiers read="true" write="true" optional="false" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.String" qualifier="indexName">
                    <modifiers read="true" write="true" optional="false" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.String" qualifier="status">
                    <modifiers read="true" write="true"  optional="false" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.String" qualifier="message">
                    <modifiers read="true" write="true"  optional="true" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.Integer" qualifier="code">
                    <modifiers read="true" write="true"  optional="true" />
                    <persistence type="property" />
                </attribute>
                <attribute qualifier="timeStamp" type="java.util.Date">
                    <modifiers read="true" write="true"  optional="false" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.Boolean" qualifier="isDelta">
                    <modifiers read="true" write="true"  optional="false" />
                    <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                    <persistence type="property" />
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="SolrFacetSearchConfig" autocreate="false" generate="false">
            <attributes>
                <attribute qualifier="unbxdSiteKey" type="java.lang.String">
                    <description>Unbxd site key</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="unbxdSecretKey" type="java.lang.String">
                    <description>Unbxd secret key</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="unbxdApiKey" type="java.lang.String">
                    <description>Unbxd api key</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="unbxdDomain" type="java.lang.String">
                    <description>Unbxd feed domain</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="unbxdSearchEnabled" type="boolean">
                    <description>Enable/disable Unbxd search</description>
                    <persistence type="property"/>
                    <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                </attribute>
                <attribute qualifier="unbxdAutosuggestEnabled" type="boolean">
                    <description>Enable/disable Unbxd auto suggest</description>
                    <persistence type="property"/>
                    <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                </attribute>
                <attribute qualifier="unbxdCategoryEnabled" type="boolean">
                    <description>Enable/disable Unbxd category</description>
                    <persistence type="property"/>
                    <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                </attribute>
                <attribute qualifier="unbxdSearchConfig" type="java.lang.String">
                    <description>Unbxd search config</description>
                    <persistence type="property">
                        <columntype>
                            <value>HYBRIS.LONG_STRING</value>
                        </columntype>
                    </persistence>
                </attribute>
                <attribute qualifier="unbxdAutosuggestConfig" type="java.lang.String">
                    <description>Unbxd auto suggest config</description>
                    <persistence type="property">
                        <columntype>
                            <value>HYBRIS.LONG_STRING</value>
                        </columntype>
                    </persistence>
                </attribute>
                <attribute qualifier="unbxdFeedPush" type="boolean">
                    <description>Enable/disable Unbxd feed push</description>
                    <persistence type="property"/>
                    <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                </attribute>
                <attribute type="UnbxdIndexerJobList" qualifier="unbxdIndexerJobList">
                    <description>Dynamic attribute to list Unbxd indexer jobs</description>
                    <persistence type="dynamic" attributeHandler="unbxdIndexerJobListAttribute" />
                </attribute>
            </attributes>
        </itemtype>
        <itemtype code="UnbxdIndexerCronJob"  extends="SolrIndexerCronJob">
        </itemtype>

    </itemtypes>
</items>
